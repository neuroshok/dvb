<%view header %>
    <%view menu %>

    <%c++
        auto resources=@@.get<std::vector<std::string>>("resources");
    %>
    <section id="content">
        <%c++ for(const auto& resource : resources) { %>
            <div>
                <h1>{% resource.name %}</h1>
                {% resource.data %}
            </div>
        <%c++ } %>
    </section>

<%view footer %>